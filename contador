algoritmo "contador"
// Função :
// Autor :
// Data : 10/06/2021
// Seção de Declarações 
var
   contador, valor, salto: Inteiro
inicio
      contador <- 0
      Escreva ("Quer contar ate quanto? ")
      Leia (valor)
      Escreva ("Qual o valor do salto? ")
      Leia (salto)
      Enquanto (contador <= valor) faca
               Escreval (contador)
               contador <- contador + salto
      FimEnquanto
      Escreval ("Terminei de contar")
// Seção de Comandos 
fimalgoritmo
